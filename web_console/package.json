{
  "name": "fedlearner-web-console",
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "build:image": "docker build -t fedlearner-web-console .",
    "cov": "nyc npm run test",
    "dev": "nodemon bootstrap.js",
    "lint": "eslint . --ext .js,.jsx --fix",
    "start": "node bootstrap.js",
    "test": "cross-env NODE_ENV=test mocha tests/*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        0,
        "always"
      ],
      "subject-case": [
        0,
        "always"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js|jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "mocha": {
    "bail": true,
    "timeout": 5000
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "components/*",
      "pages/*",
      "tests/*"
    ]
  },
  "nyc": {
    "exclude": [
      "utils/index.js"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "skip-full": true
  },
  "engines": {
    "node": ">= 10.9.0"
  },
  "author": "Bytedance Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bytedance/fedlearner.git"
  },
  "dependencies": {
    "@koa/router": "8.0.8",
    "@zeit-ui/react": "1.6.2",
    "@zeit-ui/react-icons": "1.2.2",
    "dayjs": "1.8.28",
    "get-port": "5.1.1",
    "got": "11.3.0",
    "js-yaml": "3.14.0",
    "koa": "2.12.0",
    "koa-bodyparser": "4.3.0",
    "koa-json": "2.0.2",
    "koa-onerror": "4.1.0",
    "koa-pino-logger": "3.0.0",
    "koa-session": "6.0.0",
    "mysql2": "2.1.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "sequelize": "5.21.12",
    "styled-jsx": "3.3.0",
    "swr": "0.2.2",
    "ylru": "1.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.0",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "mocha": "6.2.3",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "supertest": "4.0.2"
  }
}
