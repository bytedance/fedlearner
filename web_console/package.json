{
  "name": "fedlearner-web-console",
  "version": "0.0.0",
  "scripts": {
    "build": "next build",
    "build:image": "docker build -t fedlearner-web-console .",
    "cov": "nyc npm run test",
    "dev": "nodemon bootstrap.js",
    "lint": "eslint . --ext .js,.jsx --fix",
    "start": "node bootstrap.js",
    "test": "cross-env NODE_ENV=test mocha -r esm tests/*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-case": [
        0,
        "always"
      ],
      "subject-case": [
        0,
        "always"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js|jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "mocha": {
    "bail": true,
    "timeout": 5000
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "components/*",
      "pages/*",
      "tests/*"
    ]
  },
  "nyc": {
    "exclude": [
      "*.config.js",
      "tests/*",
      "utils/index.js",
      "server.js"
    ],
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80,
    "check-coverage": true,
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "skip-full": true
  },
  "engines": {
    "node": ">= 10.9.0"
  },
  "author": "Bytedance Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bytedance/fedlearner.git"
  },
  "dependencies": {
    "@geist-ui/react-icons": "1.0.0",
    "@grpc/grpc-js": "1.1.1",
    "@grpc/proto-loader": "0.5.4",
    "@koa/router": "8.0.8",
    "@zeit-ui/react": "1.6.2",
    "@zeit-ui/react-icons": "1.2.2",
    "autodll-webpack-plugin": "0.4.2",
    "dayjs": "1.8.28",
    "immer": "7.0.7",
    "js-yaml": "3.14.0",
    "koa": "2.12.0",
    "koa-bodyparser": "4.3.0",
    "koa-json": "2.0.2",
    "koa-onerror": "4.1.0",
    "koa-pino-logger": "3.0.0",
    "koa-session": "6.0.0",
    "ky": "0.20.0",
    "ky-universal": "0.7.0",
    "lodash": "4.17.15",
    "mysql2": "2.1.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-toastify": "6.0.8",
    "sequelize": "5.21.12",
    "sequelize-cli": "^6.2.0",
    "sockjs-client": "1.4.0",
    "styled-jsx": "3.3.0",
    "swr": "0.2.3",
    "xterm": "4.7.0",
    "xterm-addon-fit": "0.4.0",
    "ylru": "1.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.0",
    "cross-env": "7.0.2",
    "eslint": "7.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "lint-staged": "10.2.9",
    "mocha": "6.2.3",
    "nodemon": "2.0.4",
    "nyc": "15.1.0",
    "sequelize-cli": "6.2.0",
    "supertest": "4.0.2"
  }
}
